{
  "name": "rx-paired-server",
  "version": "0.0.1",
  "description": "Server for RxPaired: the lightweight, RxPlayer-oriented, remote inspector",
  "main": "./build/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc --project .",
    "run": "node ./build/main.js",
