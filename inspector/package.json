{
  "name": "rx-paired-inspector",
  "version": "0.2.1",
  "description": "Web inspector interface for RxPaired: the lightweight, RxPlayer-oriented, remote inspector",
  "main": "inspector.js",
  "scripts": {
    "build": "./build.mjs",
    "build:min": "./build.mjs -m",
    "build:watch": "./build.mjs -w",
    "build:watch:min": "./build.mjs -w -m",
    "check": "tsc --noEmit --project .",
    "fmt": "prettier --write .",
    "fmt:check": "prettier . --check",
    "lint": "eslint src -c .eslintrc.js",
    "serve": "node ../utils/static_http_server.mjs --include-inspector-files"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "8.26.1",
    "eslint": "9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.7"
  },
  "author": "peaberberian",
  "license": "MIT"
}
